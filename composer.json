{
  "name": "ticketswap/payment-przelewy24-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony2 Bundle that provides access to the Przelewy24 API. Based on JMSPaymentCoreBundle.",
  "keywords": ["payment", "przelewy24"],
  "homepage": "https://github.com/TicketSwap/PaymentPrzelewy24Bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "TicketSwap",
      "email": "info@ticketswap.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "http://github.com/TicketSwap/omnipay-przelewy24"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "symfony/framework-bundle": "~2.0 || ~3.0",
    "jms/payment-core-bundle": "~1.0",
    "psr/log": "~1.0",
    "doctrine/orm": "~2.5",
    "ticketswap/omnipay-przelewy24": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": { "TicketSwap\\Payment\\Przelewy24Bundle\\": "src" }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
